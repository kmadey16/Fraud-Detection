   "outputs": [],
   "source": [
    "flagged = df[df['isFlaggedFraud'] == 1]\n",
    "unflagged = df[df['isFlaggedFraud'] == 0]\n",
    "fraud = df[df['isFraud'] == 1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [
