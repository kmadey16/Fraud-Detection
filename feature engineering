    "fig, ax = plt.subplots(figsize=(8,6))\n",
    "sns.barplot(x= transaction_types, y=df.groupby('type')['isFraud'].sum())\n",
    "plt.title('Fraud transactions by type')\n",
    "plt.xlabel('type')\n",
    "plt.ylabel('count')\n",
    "for p in ax.patches:\n",
